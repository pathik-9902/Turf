{
  "expo": {
    "name": "turf",
    "slug": "turf",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {}
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./images/download.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    }
  }
}
